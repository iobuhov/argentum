{
    "name": "@xxx/argentum-ui",
    "version": "0.1.0-alpha.0",
    "license": "Apache-2.0",
    "type": "module",
    "exports": {
        "./*": "./dist/*.js"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/*"
            ]
        }
    },
    "files": [
        "dist",
        "!*.map"
    ],
    "devDependencies": {
        "@chromatic-com/storybook": "^1.1.9",
        "@storybook/addon-essentials": "8.0.0-beta.4",
        "@storybook/addon-interactions": "8.0.0-beta.4",
        "@storybook/addon-links": "8.0.0-beta.4",
        "@storybook/blocks": "8.0.0-beta.4",
        "@storybook/react": "8.0.0-beta.4",
        "@storybook/react-vite": "8.0.0-beta.4",
        "@storybook/test": "8.0.0-beta.4",
        "@types/react": "^18.2.57",
        "concurrently": "^8.2.2",
        "esbuild": "^0.20.1",
        "react": "^18.2.0",
        "storybook": "8.0.0-beta.4",
        "typescript": "^5.3.3"
    },
    "scripts": {
        "build:es": "esbuild ./primitives/**/* ./primitives/* --outdir=dist --outbase=primitives --log-level=info",
        "build": "pnpm build:es && tsc",
        "dev:es": "pnpm build:es --watch",
        "dev:ts": "tsc --watch",
        "dev": "concurrently --raw pnpm:dev:*",
        "lint:ts": "tsc",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    }
}
